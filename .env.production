NODE_ENV=production
REACT_APP_API_URL=https://api.fry-diagnosis-tool.com
REACT_APP_SOCKET_URL=https://api.fry-diagnosis-tool.com
REACT_APP_ENABLE_PERFORMANCE_MONITORING=false
REACT_APP_LOG_LEVEL=error
GENERATE_SOURCEMAP=false
BUILD_PATH=build

# Production Encryption Key
VITE_REACT_APP_

# Feature Flags
REACT_APP_MONITORING_DASHBOARD=true
REACT_APP_UX_TESTING=true
REACT_APP_PERFORMANCE_WIDGET=true

# Analytics and Tracking
REACT_APP_ANALYTICS_ID=GA-XXXXXXXXX
REACT_APP_ERROR_REPORTING=true

# Performance Configuration
REACT_APP_LAZY_LOADING=true
REACT_APP_CODE_SPLITTING=true
REACT_APP_SERVICE_WORKER=true

# Security
REACT_APP_SECURE_HEADERS=true
REACT_APP_CSP_ENABLED=true
