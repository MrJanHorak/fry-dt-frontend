{
  "name": "fry-dt-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:staging": "NODE_ENV=staging vite build",
    "build:production": "NODE_ENV=production vite build",
    "build:analyze": "ANALYZE_BUNDLE=true vite build",
    "preview": "vite preview",
    "preview:dist": "vite preview --port 3000",
    "lint": "eslint src --ext .js,.jsx --fix",
    "lint:check": "eslint src --ext .js,.jsx",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "optimize": "node ../build-optimizer.js",
    "serve": "serve -s dist -l 3000"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "crypto-js": "^4.1.1",
    "react": "^18.2.0",
    "react-collapsible": "^2.10.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-new-window": "^1.0.1",
    "react-qr-code": "^2.0.11",
    "react-qr-reader": "^3.0.0-beta-1",
    "react-router-dom": "^6.6.1",
    "react-select": "^5.8.0",
    "react-speech-kit": "^3.0.1",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "^0.28.0",
    "@vitest/ui": "^0.28.0",
    "eslint": "^8.32.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "imagemin": "^8.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "serve": "^14.0.1",
    "terser": "^5.39.2",
    "vite": "^4.0.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^0.28.0"
  }
}
