NODE_ENV=staging
REACT_APP_API_URL=https://staging-api.fry-diagnosis-tool.com
REACT_APP_SOCKET_URL=https://staging-api.fry-diagnosis-tool.com
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_LOG_LEVEL=warn
GENERATE_SOURCEMAP=true

# Staging Encryption Key
VITE_REACT_APP_ENCRYPTKEY=StagingSecureEncryptionKeyReplace2024

# Feature Flags
REACT_APP_MONITORING_DASHBOARD=true
REACT_APP_UX_TESTING=true
REACT_APP_PERFORMANCE_WIDGET=true

# Analytics and Tracking (Staging)
REACT_APP_ANALYTICS_ID=GA-STAGING-XXXXXXX
REACT_APP_ERROR_REPORTING=true

# Performance Configuration
REACT_APP_LAZY_LOADING=true
REACT_APP_CODE_SPLITTING=true
REACT_APP_SERVICE_WORKER=false

# Security
REACT_APP_SECURE_HEADERS=true
REACT_APP_CSP_ENABLED=true

# Staging Specific
REACT_APP_DEBUG_MODE=true
REACT_APP_TEST_MODE=true
